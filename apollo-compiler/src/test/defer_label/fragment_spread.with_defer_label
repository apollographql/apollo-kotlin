query TestQuery {
  hero {
    ...heroFragment @defer(label: "query:TestQuery:0")
    ...heroFragment @defer(label: "alreadySet")
    ...heroFragment @defer(if: true, label: "query:TestQuery:1")
    ...heroFragment @defer(label: "alreadySet", if: true)
  }
}

fragment heroFragment on Character {
  name
}
