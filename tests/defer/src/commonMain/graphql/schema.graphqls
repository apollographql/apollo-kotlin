type Query {
  computers: [Computer!]!
}

type Mutation {
  computers: [Computer!]!
}

type Subscription {
  count(to: Int!): Counter!
}

type Counter {
  value: Int!
  valueTimesTwo: Int!
}

type Computer {
  id: ID!
  cpu: String!
  year: Int!
  screen: Screen!
}

type Screen {
  resolution: String!
  isColor: Boolean!
}
