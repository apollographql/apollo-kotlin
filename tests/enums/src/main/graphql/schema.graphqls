type Query {
  direction: Direction
  gravity: Gravity
  reserved: Reserved
}

enum Direction {
  NORTH,
  north @deprecated,
  SOUTH,
  EAST,
  WEST
}

enum Gravity {
  TOP,
  # fails with java.lang.AssertionError: Class with incorrect id found: expected enums/type/Gravity.TOP, actual enums/type/Gravity.top
  # See https://youtrack.jetbrains.com/issue/KT-49352
  # top @deprecated,
  top2 @deprecated,
  BOTTOM,
  LEFT,
  RIGHT
}

# Value names should be escaped with _ prefixes
enum Reserved {
  name,
  ordinal,
}
