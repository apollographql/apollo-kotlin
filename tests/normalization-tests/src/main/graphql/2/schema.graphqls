type Query {
  viewer: Viewer!
  home: Home!
  users(includeAdmins: Boolean!): [User!]!
}

type Viewer {
  libraries(limit: Int): [Library!]!
}

type Book {
  id: String!
  name: String!
  year: Int!
  author: Author!
}

type Library {
  id: String!
  name: String!
  books: [Book]!
}

type Author {
  id: String!
  name: String!
}

type Home {
  address: String
}

type User {
  id: String!
  name: String!
  email: String!
}
